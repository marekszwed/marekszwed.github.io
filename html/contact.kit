<!DOCTYPE html>
<html lang="pl">
	<head>
		<meta charset="UTF-8" />
		<meta http-equiv="X-UA-Compatible" content="IE=edge" />
		<meta name="viewport" content="width=device-width, initial-scale=1.0" />
		<title>Kontakt</title>
		<script
			src="https://kit.fontawesome.com/f0de3a3824.js"
			crossorigin="anonymous"
		></script>
		<link
			href="https://fonts.googleapis.com/css2?family=Raleway:wght@100;200;400;500;600;700&display=swap"
			rel="stylesheet"
		/>
		<link rel="stylesheet" href="/dist/css/style.min.css" />
	</head>
	<body>
		<header class="header">
			<!-- @include nav.kit -->
		</header>
		<main class="main-contact">
			<div class="wrapper">
				<!-- @include form.kit -->

				<div class="contact">
					<ul class="contact__items">
						<li class="contact__item--name">Kontakt:</li>
						<li class="contact__item">
							<i class="fa-solid fa-phone icon"></i>

							<p class="contact__item--info">000-000-000</p>
						</li>
						<li class="contact__item">
							<i class="fa-solid fa-envelope icon"></i>
							<p class="contact__item--info">forest@group.pl</p>
						</li>
						<li class="contact__item">
							<i class="fa-solid fa-location-dot icon"></i>

							<p class="contact__item--info">Jasnogórska 2 Kraków</p>
						</li>
					</ul>
				</div>

				<div class="google">
					<div class="google__map">
						<iframe
							src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d1522.0970896155022!2d19.892792626388776!3d50.08905827615352!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x47165a326d015c3b%3A0x47a12cd367a1f34e!2sRestauracja%20McDonald&#39;s!5e0!3m2!1spl!2spl!4v1687360020898!5m2!1spl!2spl"
							width="800"
							height="450"
							style="border: 0"
							allowfullscreen=""
							loading="lazy"
							referrerpolicy="no-referrer-when-downgrade">
						</iframe>
					</div>
				</div>
			</div>
		</main>

		<!-- @include footer.kit -->

		<script src="/dist/js/main.min.js"></script>
	</body>
</html>
